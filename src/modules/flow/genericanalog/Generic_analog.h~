/* Header file for generic analog sensor
 * contains function similar to what a regular UPM driver would have
 */

/* Struct for the generic analog sensor
 *
 */
struct _upm_generic_analog {
	struct sol_flow_node *node;
	struct sol_timeout *timer;
	struct sol_aio *aio;
	int	pin;
	int	mask;
	int	ret_data;
};

//typedef struct _upm_generic_analog* upm_generic_analog;

/* generic init function
 *
 */
upm_generic_analog upm_generic_analog_sensor_init(int pin);

/* generic close function
 *
 */
void upm_generic_analog_sensor_close();

/* Generic analog read value function
 * 
 */
int upm_generic_analog_read_value();
